{
  "data_provider": {
    "window_size": 400,
    "window_height": 1,
    "step_size": 200,
    "signals": ["accel_global", "velocity", "position"],
    "channels": [[2], [2], [2]],
    "labels": ["Jump", "Move", "Sit", "Stand", "Walk"],
    "shuffle": false,
    "normalize": "STANDARD"
  },
  "model": {
    "type": "CNN",
    "depth": 60,
    "kernel": 60,
    "hidden": 1000,
    "learning_rate": 0.001
  },
  "training": {
    "batch_size": 100,
    "epochs": 8
  }
}